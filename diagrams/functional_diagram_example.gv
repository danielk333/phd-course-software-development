digraph {
    rankdir="LR";
    dpi=300;
    data [
        label = "Raw data"
        shape = plain
    ];

    node [ shape=rect ];
    proc [ label="Hard target matched filtering" ];
    clust [ label="Event clustering" ];
    noise [ label="Reciver noise calibration" ];

    track [
        label="Tracklets"
        shape = plain
    ];
    data -> proc -> clust -> track;
    proc -> noise;
}
